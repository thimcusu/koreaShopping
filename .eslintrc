{
    "extend": [ "eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
    "plugins": [ "react" ],
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true,
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
    },
    "rules": {
        "react/react-in-jsx-scope": "off"
    }
}